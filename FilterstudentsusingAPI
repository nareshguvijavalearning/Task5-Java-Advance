

import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;

public class Students {

	public static void main(String[] args) {

		List<String>student = Arrays.asList("naresh","ankit","aathi","kumar","siva","azhagu","nithish","nivin","deeepak","akash");

		
		
		student.stream().filter(x->x.startsWith("a")).forEach(System.out::println);
	
		
		
	}

}


Output:

ankit
aathi
azhagu
akash

